{
  "name": "spatie/laravel-permission",
  "description": "Permission handling for Laravel 5.1 and up",
  "keywords": [
    "spatie",
    "laravel",
    "permission",
    "acl",
    "security"
  ],
  "homepage": "https://github.com/cytonn/laravel-permission",
  "license": "MIT",
  "authors": [
    {
      "name": "Freek Van der Herten",
      "email": "freek@spatie.be",
      "homepage": "https://spatie.be",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "laravel/framework": "^8.0",
    "illuminate/contracts": "^5.8"
  },
  "require-dev": {
    "orchestra/testbench": "^3.3.0 <3.8.0",
    "phpunit/phpunit": "^5.7.8|~6.0|^7.0"
  },
  "autoload": {
    "psr-4": {
      "Spatie\\Permission\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Spatie\\Permission\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Spatie\\Permission\\PermissionServiceProvider"
      ]
    }
  }
}
